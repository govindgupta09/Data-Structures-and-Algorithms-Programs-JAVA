//Function to check whether a Binary Tree is BST or not.


public class Solution
{
    //Function to check whether a Binary Tree is BST or not.
    Node prev = null;
    boolean isBST(Node root)
    {
        if(root !=null){
           if(!isBST(root.left))
           return false;
           if(prev !=null && root.data <=prev.data)
           return false;
           prev = root;
           return isBST(root.right);
       }
       return true;
    }
}
